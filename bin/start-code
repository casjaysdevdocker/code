# @@sudo/root        :  no
# @@Template         :  other/start-service
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
[ "$1" = "" ] && shift 1
[ "$1" = " " ] && shift 1
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
WORK_DIR="${1:-/data}"
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
code -wn "$WORK_DIR"
